<body>

<%- include partials/navlogin.ejs%>
<!-- MultiStep Form -->
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <form id="msform" action="/users/login_user" method = "post"  encType="multipart/form-data">
            <!-- progressbar -->


            <fieldset>

                <h2 class="fs-title">Lets Connect</h2>
                <h3 class="fs-subtitle">Login to access your profile and get connected with your classmates.</h3>

                <input type="text" id = "c" name="email" placeholder="Email" required/>
                <input type="password"  id = "d" name="password" placeholder="Password" required/>


                <input type='submit' value='Submit' />


            </fieldset>

        </form>

    </div>
</div>
<!-- /.MultiStep Form -->
</body>
</html>



<script>
    $("#msform").validate();
</script>




</html>