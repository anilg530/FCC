<!-- MultiStep Form -->


<!DOCTYPE HTML>
<html>
<head>

    <link href="stylesheets/bootstrap.css" rel="stylesheet">
    <link href="stylesheets/custom1.css" rel="stylesheet">
    <script src="js/jquery.min.js"></script>
    <script src="js/main.js"></script>
    <script src = "https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"> </script>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>


</head>

<body>


<!-- MultiStep Form -->
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <form id="msform" action="/users/new_user" method = "post"  encType="multipart/form-data">
            <!-- progressbar -->


            <fieldset>

                <h2 class="fs-title">Lets Connect</h2>
                <h3 class="fs-subtitle">Tell us more about you and upload a screenshot of your schedule.</h3>
                <input type="text"  id = "a" name="firstName" placeholder="First Name" required/>
                <input type="text" id  = "b" name="lastName" placeholder="Last Name" required/>
                <input type="text" id = "c" name="email" placeholder="Email" required/>
                <input type="password"  id = "d" name="password" placeholder="Password" required/>
                <input type="password" name="confirm" placeholder="Confirm Password" required/>
                <input type="file" name="sampleFile" id ="sampleFile" />
                <input type='submit' value='Submit' />


            </fieldset>
            <fieldset>
                <h2 class="fs-title">Upload Schedule</h2>
                <h3 class="fs-subtitle">Take screenshot of your schedule</h3>

                <input type="button" name="previous" class="previous action-button-previous" value="Previous"/>
                <input type="button" name="next" class="next action-button" value="Next"/>
            </fieldset>
            <fieldset>
                <h2 class="fs-title">Confirmation Stuff</h2>
                <h3 class="fs-subtitle">Fill in your credentials</h3>

                <input type="button" name="previous" class="previous action-button-previous" value="Previous"/>
                <input type="submit" name="submit" class="submit action-button" value="Submit"/>
            </fieldset>
        </form>

    </div>
</div>
<!-- /.MultiStep Form -->
</body>
</html>



<script>
    $("#msform").validate();
</script>




</html>