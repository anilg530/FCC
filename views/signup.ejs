<!-- MultiStep Form -->


<!DOCTYPE HTML>
<html>
<head>

    <link href="stylesheets/bootstrap.css" rel="stylesheet">
    <link href="stylesheets/custom4.css" rel="stylesheet">
    <script src="js/jquery.min.js"></script>
    <script src="js/main.js"></script>
    <script src = "https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"> </script>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>


</head>

<body>
<%- include partials/navsignup.ejs%>

<!-- MultiStep Form -->
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <form id="msform" action="/new_user" method = "post">
            <!-- progressbar -->


            <fieldset>

                <h2 class="fs-title">Lets Connect</h2>
                <h3 class="fs-subtitle">Tell us more about you.</h3>
                <% if (error) { %>
                <p style = "color: red">*<%= error %></p>
                <% } %>
                <input type="text"  id = "a" name="firstName" placeholder="First Name" required/>
                <input type="text" id  = "b" name="lastName" placeholder="Last Name" required/>
                <input type="email" id = "c" name="email" placeholder="Email" required/>
                <input type="password"  id = "d" name="password" placeholder="Password" required/>
                <input type="password" name="confirm" placeholder="Confirm Password" required/>
                <button class = "btn-block btn-success btn-lg" type = 'submit'> Next </button>


            </fieldset>

        </form>

    </div>
</div>

</body>
</html>




</html>